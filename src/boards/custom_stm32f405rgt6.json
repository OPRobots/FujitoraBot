{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32F4 -DSTM32F415xx -DSTM32F40_41xxx -DBOARD_discovery_f4",
    "f_cpu": "168000000L",
    "ldscript": "stm32f405x6.ld",
    "mcu": "stm32f415rgt",
    "variant": "stm32f415xx"
  },
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32F415RG",
    "openocd_target": "stm32f4x",
    "svd_path": "STM32F41x.svd"
  },
  "frameworks": [
    "libopencm3",
    "cmsis",
    "spl",
    "stm32cube"
  ],
  "name": "1Bitsy",
  "upload": {
    "maximum_ram_size": 131072,
    "maximum_size": 1048576,
    "protocol": "blackmagic",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic"
    ]
  },
  "url": "http://1bitsy.org",
  "vendor": "1BitSquared"
}